<!-- home.handlebars -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <h1>Lista de Productos</h1>
    <ul>
        {{#each productos}}
            <li>
                <strong>{{this.title}}</strong> - {{this.description}} - {{this.price}} - {{this.category}} - {{this.thumbnail}} - {{this.stock}} - {{this.code}}
            </li>
        {{/each}}
    </ul>
</body>
</html>
